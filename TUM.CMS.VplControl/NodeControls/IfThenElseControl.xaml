<Window x:Class="TUM.CMS.VplControl.NodeControls.IfThenElseControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TUM.CMS.VplControl.NodeControls"
        mc:Ignorable="d"
        Title="IfThenElseControl" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="1">
            <GroupBox Header="Parameters">
                <StackPanel>
                    <TextBlock Text="Number of Inputs"/>
                    <TextBox />
                </StackPanel>
            </GroupBox>
        </StackPanel>
        
    </Grid>
</Window>
